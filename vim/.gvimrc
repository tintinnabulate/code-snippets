colo desert

set guifont=Consolas:h12:cANSI
set guicursor+=a:blinkon0

syntax on
filetype plugin indent on

let mapleader=','

set nobackup
set nowritebackup

set nu

set hlsearch
set incsearch

set expandtab
set tabstop=4
set shiftwidth=4
set autoindent
set backspace=indent,eol,start

set enc=utf-8
set fileencoding=utf-8
set fileencodings=ucs-bom,utf8,prc
